{
	"name": "latchel-app",
	"description": "A Simple Blog",
	"keywords": [
		"blog",
		"angularjs",
		"php",
		"web"
	],
	"homepage": "http://github.com/ideaguy3d/latchel",
	"license": "MIT",
	"authors": [
		{
			"name": "Julius",
			"homepage": "http://github.com/ideaguy3d"
		}
	],
	"autoload": {
		"psr-4": {
			"Latchel\\": ["src/controllers", "src/models", "src/structs"]
		}
	},
	"require": {
		"ext-finfo": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-xsl": "*",
        "ext-mbstring": "*",
        "ext-xdebug": "*",
        "ext-curl": "*",
        "ext-libxml": "*"
    },
	"config": {
		"process-timeout": 0
	},
	"scripts": {
		"start": "php -S localhost:8080 -t public",
		"test": "phpunit"
	}
}
